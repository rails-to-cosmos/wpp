{
    "actions": [
        {
            "type": "FastDownload",
            "name": "_webpage",
            "target": "https://news.ycombinator.com"
        },
        {
            "type": "parse",
            "name": "_item",
            "target": "_webpage",
            "data": {
                "$": ".athing[outerHTML]"
            }
        },
        {
            "type": "parse",
            "name": "id",
            "target": "_item",
            "data": {
                "$": "[id]"
            }
        },
        {
            "type": "parse",
            "name": "title",
            "target": "_item",
            "data": {
                "$": ">.title>.storylink"
            }
        }
    ]
}
